{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "globals": {
    "sap": true,
    "jQuery": true
  },
  "rules": {
    "mocha/no-mocha-arrows": 0,
    "block-scoped-var": 1,
    "keyword-spacing": 2,
    "space-unary-ops": 2,
    "camelcase": 1,
    "consistent-return": 1,
    "no-warning-comments": 1,
    "default-case": 1,
    "no-console": 2,
    "no-unused-vars": 2,
    "no-debugger": 2,
    "no-trailing-spaces": [
      2,
      {
        "ignoreComments": true
      }
    ],
    "semi": [
      1,
      "always"
    ],
    "quotes": [
      1,
      "double"
    ],
    "key-spacing": [
      1,
      {
        "beforeColon": false
      }
    ],
    "comma-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "no-shadow": 2,
    "no-irregular-whitespace": 2
  },
  "plugins": [
    "@sap/ui5-jsdocs",
    "mocha",
    "wdio"
  ],
  "extends": [
    "plugin:@sap/ui5-jsdocs/recommended",
    "eslint:recommended",
    "plugin:mocha/recommended",
    "plugin:wdio/recommended"
  ]
}
